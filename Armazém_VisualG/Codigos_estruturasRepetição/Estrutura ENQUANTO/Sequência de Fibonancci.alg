algoritmo "SequênciaDeFibonacci"

var
until,i,j,z:inteiro
inicio
    i <- 0
    j <- 1
    EscrevaL ("Sequência de Fibonacci")
    Escreva  ("Inserir valor limite ")
    Leia (until)

          Enquanto (i  <= until) faca
                   EscrevaL(i)
                   i <- i + j
                   j <- i - j

          FimEnquanto



fimalgoritmo
